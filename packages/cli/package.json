{
    "name": "@accessibility-toolkit/cli",
    "version": "0.1.0",
    "description": "CLI tool for accessibility testing with optional React and AI auditor support",
    "main": "dist/index.js",
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./dist/index.js": "./dist/index.js"
    },
    "bin": {
        "a11y-toolkit": "./bin/cli.js"
    },
    "files": [
        "dist",
        "bin"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "start": "node ./bin/cli.js",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "keywords": [
        "accessibility",
        "a11y",
        "cli",
        "wcag",
        "axe"
    ],
    "license": "MIT",
    "dependencies": {
        "@accessibility-toolkit/core": "workspace:*",
        "@accessibility-toolkit/react": "workspace:*",
        "@accessibility-toolkit/ai-auditor": "workspace:*",
        "chalk": "^5.3.0",
        "dotenv": "^17.2.3",
        "ink": "^4.4.1",
        "ink-spinner": "^5.0.0",
        "meow": "^13.1.0",
        "react": "^18.3.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/react": "^18.3.1",
        "ink-testing-library": "^4.0.0",
        "typescript": "^5.3.0",
        "vitest": "^4.0.13"
    }
}
